async def get_pre(client, message):
    '''Gets the prefix for the guild'''
    try:
        result = await client.db.config.find_one({'_id': str(message.guild.id)})
    except AttributeError:
        return '^'
    if not result or not result.get('prefix'):
        return '^'
    return result

